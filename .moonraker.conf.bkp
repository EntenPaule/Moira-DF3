[server]
host = 0.0.0.0
port = 7125
enable_debug_logging = False

[authorization]
cors_domains = 
	http://mainsail
trusted_clients = 
	10.0.0.0/8
	127.0.0.0/8
	192.168.2.0/24

[octoprint_compat]

[history]

[update_manager]

[update_manager mainsail]
type = web
repo = mainsail-crew/mainsail
path = ~/mainsail

[file_manager]
config_path = ~/klipper_config
log_path = /tmp

[data_store]
temperature_store_size = 600
gcode_store_size = 1000

